<!--index.wxml-->
<wxs src="../common/page.wxs" module="page"></wxs>
<include src="/template/render/render.wxml" />
<view class="cu-load pull-down text-gray"></view>
<view class='container page-top' data-width="{{ScreenHeight}}" bindtouchmove="{{page.touchMove}}" bindtouchend="{{page.touchEnd}}" bindtouchstart="{{page.touchStart}}" change:prop="{{page.propObserver}}" prop="{{pageStats}}">
  <block wx:for="{{list}}" wx:key="index">
    <template is="{{item.type}}" data="{{...item, tidx: index}}"></template>
  </block>
</view>
<view class="cu-load pull-up text-gray"></view>